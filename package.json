{
	"name": "@kekalma/language-selector",
	"version": "0.0.8",
	"description": "Language selector component for React",
	"author": "kissato70",
	"license": "MIT",
	"funding": {
		"type": "individual",
		"url": "https://bit.ly/kissato70_paypal_donate"
	},
	"repository": "kissato70/kekalma-language-selector",
	"main": "dist/language-selector.js",
	"source": "src/language-selector.tsx",
	"engines": {
		"node": ">=10"
	},
	"workspaces": [
		"example"
	],
	"keywords": [
		"react",
		"language",
		"dropdown",
		"selector",
		"component",
		"multi-language",
		"typescript"
	],
	"scripts": {
		"start": "yarn install && node ./.bin/init.js && yarn install && yarn dist && yarn _predeploy && cd ..",
		"dev": "rollup --config rollup.config.dev.js -w",
		"dist": "rollup --config rollup.config.dist.js",
		"dev:microbundle": "microbundle-crl watch --no-compress --css inline --format modern,cjs",
		"dist:microbundle": "microbundle-crl --compress --format modern,cjs",
		"serve": "cd example && yarn serve",
		"_predeploy": "cd example && yarn install && yarn run build && cd .."
	},
	"peerDependencies": {
		"react": "^16.0.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^20.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.0.4",
		"@rollup/plugin-replace": "^3.0.0",
		"@types/node": "^16.6.1",
		"@types/react": "^17.0.18",
		"@types/react-dom": "^17.0.9",
		"@types/react-router-dom": "^5.1.8",
		"microbundle-crl": "^0.13.11",
		"postcss": "^8.3.6",
		"postcss-import": "^14.0.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "^4.0.3",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-copy": "^3.4.0",
		"rollup-plugin-livereload": "^2.0.5",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-postcss": "^4.0.1",
		"rollup-plugin-serve": "^1.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.30.0",
		"typescript": "^4.3.5"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"national-flag-icons": "^1.0.1",
		"react-router-dom": "^5.2.0"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	}
}
